"../components/Pagination.vue\";\nimport { computed, onMounted } from \"vue\";\n\nimport { useStore } from \"vuex\";\n\nexport default {\n  components: {\n    PageTitle,\n    Pagination,\n  },\n  computed: {},\n  setup() {\n    const store = useStore();\n    const blogs = computed(() => store.getters.blogs);\n\n    onMounted(() => {\n      store.dispatch(\"getAllNews\");\n      store.commit(\"newPageTitle\", \"news\");\n    });\n    return {\n      blogs,\n    };\n  },\n};\n</script>\n\n<style lang='scss'>\n.blogs {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 20px;\n  @media (max-width: 767px) {\n    grid-template-columns: 1fr;\n  }\n  .blog {\n    img {\n      max-width: 100%;\n    }\n    .blog-content {\n      padding: 20px 115px 0;\n      text-align: center;\n      @media (max-width: 767px) {\n        padding: 20px 15px 0;\n      }\n      span {\n        font-size: 16px;\n      }\n      h3 {\n        a {\n          font-size: 25px;\n          color: #363636;\n        }\n      }\n      p {\n        font-size: 14px;\n        font-weight: 400;\n        line-height: 24px;\n        color: #626262;\n        margin-bottom: 15px;\n      }\n    }\n  }\n}\n</style>"]}]}