return {\n      blogDetails,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.just-blog {\n  width: 804px;\n  margin: auto;\n  .just-blog-img {\n    img {\n      width: 100%;\n    }\n  }\n  .just-blog-content {\n    padding-top: 85px;\n    margin-bottom: 85px;\n    h3 {\n      font-size: 29px;\n      line-height: 45px;\n      margin-bottom: 30px;\n      text-transform: capitalize;\n    }\n    p {\n      font-size: 15px;\n      color: rgb(98, 98, 98);\n      margin-bottom: 15px;\n    }\n    .quote {\n      border-left: 3px solid #363636;\n      color: #363636;\n      font-size: 27px;\n      margin: 35px 0;\n      padding: 7px 20px 15px;\n      font-family: Playfair Display, serif, serif;\n      font-style: italic;\n    }\n  }\n  @media (max-width: 991px) {\n    width: 96%;\n  }\n}\n</style>\n"],"sourceRoot":""}]}