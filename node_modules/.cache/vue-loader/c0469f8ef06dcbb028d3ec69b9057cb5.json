AAC,CAAC,CAAC,CAAC","file":"/media/abdelrahman/Projects/Vue.projects/done/mira/src/components/ScrollToTop.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"scroll-to-top\" @click=\"toTop\" ref=\"scroll\">\n    <font-awesome-icon class=\"scroll-chev\" :icon=\"['fas', 'chevron-up']\" />\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nexport default {\n  setup() {\n    const toTop = () => {\n      window.scrollTo({\n        top: 0,\n        behavior: \"smooth\",\n      });\n    };\n\n    const scroll = ref(null);\n\n    window.onscroll = function () {\n      if (window.scrollY > 500) {\n        scroll.value.style.opacity = 1;\n      } else {\n        scroll.value.style.opacity = 0;\n      }\n    };\n\n    return {\n      toTop,\n      scroll,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.scroll-to-top {\n  display: none;\n  position: fixed;\n  background: #666 none repeat scroll 0 0;\n  bottom: 54px;\n  color: #fff;\n  font-size: 23px;\n  height: 40px;\n  line-height: 34px;\n  right: 32px;\n  text-align: center;\n  width: 40px;\n  transition: 0.3s;\n  cursor: pointer;\n  z-index: 9999;\n  &:hover {\n    background-color: #222;\n  }\n  svg {\n    font-size: 14px;\n  }\n}\n</style>\n"]}]}